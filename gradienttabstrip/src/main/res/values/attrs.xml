<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GradientTabStrip">
        <attr name="gtsDivider" format="reference" />
        <attr name="gtsShowDividers">
            <flag name="none" value="0" />
            <flag name="beginning" value="1" />
            <flag name="middle" value="2" />
            <flag name="end" value="4" />
        </attr>
        <attr name="gtsDividerPadding" format="dimension" />
        <attr name="gtsCenterInterval" format="reference" />
        <attr name="gtsCenterIntervalAsItem" format="boolean" />
        <attr name="gtsCenterIntervalPadding" format="dimension" />
        <attr name="gtsClickSmoothScroll" format="boolean" />
        <attr name="gtsItemBackground" format="reference" />
        <attr name="gtsTextSize" format="dimension" />
        <attr name="gtsTextColorNormal" format="color" />
        <attr name="gtsTextColorSelected" format="color" />
        <attr name="gtsDrawablePadding" format="dimension" />
        <attr name="gtsDotCenterToViewCenterX" format="dimension" />
        <attr name="gtsDotCenterToViewCenterY" format="dimension" />
        <attr name="gtsDotCanGoOutside" format="boolean" />
        <attr name="gtsDotAutoChangeWidth" format="boolean" />
        <attr name="gtsDotColor" format="color" />
        <attr name="gtsDotBackground" format="reference" />
        <attr name="gtsDotTextSize" format="dimension" />
        <attr name="gtsDotTextColor" format="color" />
    </declare-styleable>
</resources>