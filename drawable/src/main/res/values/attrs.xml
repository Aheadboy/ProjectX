<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LineDrawable">
        <attr name="android:gravity" />
        <attr name="android:background" />
        <attr name="android:color" />
        <attr name="android:width" />
    </declare-styleable>
    <declare-styleable name="PathDrawable">
        <attr name="android:width" />
        <attr name="android:height" />
        <attr name="pdFillType" format="enum">
            <enum name="winding" value="0" />
            <enum name="evenOdd" value="1" />
            <enum name="inverseWinding" value="2" />
            <enum name="inverseEvenOdd" value="3" />
        </attr>
        <attr name="pdStyle" format="enum">
            <enum name="fill" value="0" />
            <enum name="stroke" value="1" />
            <enum name="fillAndStroke" value="2" />
        </attr>
        <attr name="android:color" />
        <attr name="pdStrokeWidth" format="dimension" />
        <attr name="android:strokeLineCap" />
        <attr name="android:strokeLineJoin" />
        <attr name="android:strokeMiterLimit" />

        <attr name="android:pathData" />
    </declare-styleable>
</resources>