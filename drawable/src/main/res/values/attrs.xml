<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LineDrawable">
        <attr name="ldBackgroundColor" format="color" />
        <attr name="ldLineColor" format="color" />
        <attr name="ldLineSize" format="dimension" />
        <attr name="ldGravity">
            <flag name="top" value="0x30" />
            <flag name="bottom" value="0x50" />
            <flag name="left" value="0x03" />
            <flag name="right" value="0x05" />
            <flag name="center_vertical" value="0x10" />
            <flag name="center_horizontal" value="0x01" />
        </attr>
    </declare-styleable>
    <declare-styleable name="MaterialProgressImageView">
        <attr name="mpiAutoStart" format="boolean" />
        <attr name="mpiShadowsCircleColor" format="color" />
        <attr name="mpiShadowsCircleRadius" format="dimension" />
        <attr name="mpiElevation" format="dimension" />
        <attr name="mpiSchemeColor1" format="color" />
        <attr name="mpiSchemeColor2" format="color" />
        <attr name="mpiSchemeColor3" format="color" />
        <attr name="mpiSchemeColor4" format="color" />
        <attr name="mpiSchemeColor5" format="color" />
    </declare-styleable>
</resources>