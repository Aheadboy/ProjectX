<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LineDrawable">
        <attr name="android:gravity" />
        <attr name="android:background" />
        <attr name="android:color" />
        <attr name="android:width" />
    </declare-styleable>
    <declare-styleable name="PathDrawable">
        <attr name="android:background" />
        <attr name="android:width" />
        <attr name="android:height" />
        <attr name="android:viewportWidth" />
        <attr name="android:viewportHeight" />
        <attr name="android:pathData" />
        <attr name="pdFillType" format="enum">
            <enum name="winding" value="0" />
            <enum name="evenOdd" value="1" />
            <enum name="inverseWinding" value="2" />
            <enum name="inverseEvenOdd" value="3" />
        </attr>
        <attr name="android:fillColor" />
        <attr name="android:strokeColor" />
        <attr name="android:strokeWidth" />
        <attr name="pdStrokeWidthScaleType">
            <enum name="average" value="0" />
            <enum name="min" value="1" />
            <enum name="max" value="2" />
            <enum name="width" value="3" />
            <enum name="height" value="4" />
            <enum name="none" value="5" />
        </attr>
        <attr name="android:strokeLineCap" />
        <attr name="android:strokeLineJoin" />
        <attr name="android:strokeMiterLimit" />
        <attr name="pdClipStroke" format="boolean" />
    </declare-styleable>
    <declare-styleable name="GridDrawable">
        <attr name="android:src" />
        <attr name="android:rowCount" />
        <attr name="android:columnCount" />
        <attr name="android:horizontalSpacing" />
        <attr name="android:verticalSpacing" />
        <attr name="android:constantSize" />
    </declare-styleable>
    <declare-styleable name="FrameDrawable">
        <attr name="android:layout_width" />
        <attr name="android:layout_height" />
        <attr name="android:padding" />
        <attr name="android:paddingStart" />
        <attr name="android:paddingTop" />
        <attr name="android:paddingEnd" />
        <attr name="android:paddingBottom" />
    </declare-styleable>
    <declare-styleable name="FrameDrawableItem">
        <attr name="android:layout_width" />
        <attr name="android:layout_height" />
        <attr name="android:layout_margin" />
        <attr name="android:layout_marginStart" />
        <attr name="android:layout_marginTop" />
        <attr name="android:layout_marginEnd" />
        <attr name="android:layout_marginBottom" />
        <attr name="android:gravity" />
        <attr name="android:id" />
        <attr name="android:drawable" />
    </declare-styleable>
    <declare-styleable name="RadialRippleAnimationDrawable">
        <attr name="android:startColor"/>
        <attr name="android:endColor"/>
    </declare-styleable>
</resources>